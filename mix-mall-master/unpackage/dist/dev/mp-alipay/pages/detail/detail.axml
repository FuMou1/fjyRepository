<view><swiper class="carousel" indicator-dots="true" circular="true" interval="3000" duration="700"><swiper-item a:for="{{data.imgList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="image-wrapper"><image class="{{(item.loaded)}}" src="{{item.src}}" mode="aspectFill" data-event-opts="{{[['load',[['imageOnLoad',['imgList',index]]]]]}}" onLoad="__e"></image></view></swiper-item></swiper><view class="scroll-view-wrapper"><scroll-view class="{{(('episode-panel')+' '+((!loaded)?'Skeleton':''))}}" scroll-x="{{true}}"><view data-event-opts="{{[['tap',[['changeEpd',[index]]]]]}}" class="{{((currentEpd===item)?'current':'')}}" onTap="__e" a:for="{{data.episodeList}}" a:for-item="item" a:for-index="index" a:key="index">{{''+item+''}}</view></scroll-view></view><view class="info"><view class="title"><text class="{{((!loaded)?'Skeleton':'')}}">{{data.title}}</text><text class="{{((!loaded)?'Skeleton':'')}}">{{data.title2}}</text></view><text class="yticon icon-xia"></text></view><view class="actions"><text data-event-opts="{{[['tap',[['share',['$event']]]]]}}" class="yticon icon-fenxiang2" onTap="__e"></text><text class="yticon icon-Group-"></text><text data-event-opts="{{[['tap',[['favorite',['$event']]]]]}}" class="{{((('yticon')+' '+'icon-shoucang')+' '+((data.favorite)?'active':''))}}" onTap="__e"></text></view><view class="guess"><view class="section-tit">猜你喜欢</view><view class="guess-list"><view class="guess-item" a:for="{{data.guessList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="image-wrapper"><image class="{{(item.loaded)}}" src="{{item.src}}" mode="aspectFill" data-event-opts="{{[['load',[['imageOnLoad',['guessList',index]]]]]}}" onLoad="__e"></image></view><text class="{{((('title')+' '+'clamp')+' '+((!loaded)?'Skeleton':''))}}">{{item.title}}</text><text class="{{(('clamp')+' '+((!loaded)?'Skeleton':''))}}">{{item.title2}}</text></view></view></view><view class="evalution"><view class="section-tit">评论</view><view class="{{(('eva-list')+' '+((!loaded)?'Skeleton':''))}}"><view class="eva-item" a:for="{{data.evaList}}" a:for-item="item" a:for-index="index" a:key="index"><image src="{{item.src}}" mode="aspectFill"></image><view class="eva-right"><text>{{item.nickname}}</text><text>{{item.time}}</text><view class="zan-box"><text>{{item.zan}}</text><text class="yticon icon-shoucang"></text></view><text class="content">{{item.content}}</text></view></view></view></view><share vue-id="3da764f6-1" contentHeight="{{580}}" shareList="{{shareList}}" ref="__r" data-ref="share" onVueInit="__l"></share></view>