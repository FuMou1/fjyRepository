<view class="content"><scroll-view class="left-aside" scroll-y="{{true}}"><view data-event-opts="{{[['tap',[['tabtap',['$0'],[[['flist','id',item.id]]]]]]]}}" class="{{((('f-item')+' '+'b-b')+' '+((item.id===currentId)?'active':''))}}" onTap="__e" a:for="{{flist}}" a:for-item="item" a:for-index="__i0__" a:key="id">{{''+item.name+''}}</view></scroll-view><scroll-view class="right-aside" scroll-with-animation="{{true}}" scroll-y="{{true}}" scroll-top="{{tabScrollTop}}" data-event-opts="{{[['scroll',[['asideScroll',['$event']]]]]}}" onScroll="__e"><view class="s-list" id="{{'main-'+item.id}}" a:for="{{slist}}" a:for-item="item" a:for-index="__i1__" a:key="id"><text class="s-item">{{item.name}}</text><view class="t-list"><block a:for="{{tlist}}" a:for-item="titem" a:for-index="__i2__" a:key="id"><block a:if="{{titem.pid===item.id}}"><view data-event-opts="{{[['tap',[['navToList',['$0','$1'],[[['slist','id',item.id,'id']],[['tlist','id',titem.id,'id']]]]]]]}}" class="t-item" onTap="__e"><image src="{{titem.picture}}"></image><text>{{titem.name}}</text></view></block></block></view></view></scroll-view></view>